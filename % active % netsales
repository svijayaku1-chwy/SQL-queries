% active % netsales
SELECT
(COUNT(DISTINCT P.CUSTOMER_ID)/(SELECT COUNT(DISTINCT CUSTOMER_ID) FROM "EDLDB"."ECOM_SANDBOX"."PREMIUM_CUSTOMER_LIST"))*100 AS PER_ACTIVE_CUS,
(SUM(NETSALES_L12M)/(SELECT SUM(NETSALES_L12M) FROM "EDLDB"."CDM"."CUSTOMER_AGGREGATE"))*100 AS PER_TOTAL_NETSALES12M

FROM CDM.CUSTOMER_AGGREGATE AS C
JOIN "EDLDB"."ECOM_SANDBOX"."PREMIUM_CUSTOMER_LIST" AS P
ON P.CUSTOMER_ID = C.CUSTOMER_ID

WHERE ORDERS_L12M > 0
;